arr1 = [1,2,3,5,4,6,7,8,9]

 var oddValues = function(array){
    return _.filter(array, function(element){ 
      return (element % 2 ) !== 0; 
    });
}

oddValues(arr1);


 console.log(oddValues(arr1));


</script>
